{
  "expo": {
    "name": "Timpul Liturgic",
    "slug": "liturgical-time",
    "version": "2.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "liturgicaltime",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F5F0E8"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.amariei26.liturgicaltime"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#F5F0E8"
      },
      "package": "com.amariei26.liturgicaltime",
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      ["expo-calendar", {
        "calendarPermission": "Timpul Liturgic necesită acces la calendar pentru sincronizarea întâlnirilor."
      }],
      ["expo-splash-screen", {
        "backgroundColor": "#F5F0E8",
        "image": "./assets/images/splash.png",
        "imageWidth": 200
      }]
    ]
  }
}
